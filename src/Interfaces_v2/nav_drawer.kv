#:include blacklist_window.kv
#:include whitelist_window.kv
#:include logs_window.kv
#:include home.kv
#:include user_management_window.kv
<Navigator>:
    NavigationDrawerIconButton:
        icon: 'home'
        text: 'Home'
        on_release: app.nav_to('home')
    NavigationDrawerIconButton:
        icon: 'tv'
        text: 'Backends'
        on_release: app.load_website('http://127.0.0.1:8761','Service resources')
    NavigationDrawerIconButton:
        icon: 'chart'
        text: 'Metrics'
        on_release: app.load_website('http://127.0.0.1:3000/d/L7frhkNWk/defendr-system-load?orgId=1','System Metrics')

    NavigationDrawerIconButton:
        icon: 'format-list-bulleted'
        text: 'System logs'
        on_release: app.nav_to('logs_window')
    NavigationDrawerIconButton:
        icon: 'shield-security'
        text: 'Blacklisting'
        on_release: app.nav_to('blacklist_window')
    NavigationDrawerIconButton:
        icon: 'shield-check'
        text: 'Whitelisting'
        on_release: app.nav_to('whitelist_window')
    NavigationDrawerIconButton:
        icon: 'face'
        text: 'User management'
        on_release: app.nav_to('user_management_window')
    NavigationDrawerIconButton:
        icon: 'arrow-left'
        text: 'Logout'
        on_release: app.nav_to('login')